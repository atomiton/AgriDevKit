<Macro name="LoadSensorDataType">
	<Argument />
		
	<Result>
		<executeQuery>
			<QueryString>
				<DeleteAll>
					<SensorDataType>
						<ID ne="" />
					</SensorDataType>
				</DeleteAll>
			</QueryString>
		</executeQuery>
		<executeQuery>
			<QueryString>
				<Create>
					<SensorDataType>
						<Name>AirTemperature</Name>
						<MCECodeName>TCB</MCECodeName>
						<Datatype>double</Datatype>
						<Unit>C</Unit>
						<DefaultValue>true</DefaultValue>
					</SensorDataType>
				</Create>
				<Create>
					<SensorDataType>
						<Name>AirHumidity</Name>
						<MCECodeName>HUMB</MCECodeName>
						<Datatype>double</Datatype>
						<Unit>%</Unit>
						<DefaultValue>true</DefaultValue>
					</SensorDataType>
				</Create>
				<Create>
					<SensorDataType>
						<Name>LeafWetness</Name>
						<MCECodeName>LW</MCECodeName>
						<Datatype>double</Datatype>
						<Unit>%</Unit>
						<DefaultValue>true</DefaultValue>
					</SensorDataType>
				</Create>
				<Create>
					<SensorDataType>
						<Name>SolarRadiation</Name>
						<MCECodeName>PAR</MCECodeName>
						<Datatype>double</Datatype>
						<Unit>w/msq</Unit>
						<DefaultValue>true</DefaultValue>
					</SensorDataType>
				</Create>
				<Create>
					<SensorDataType>
						<Name>AtmosphericPressure</Name>
						<MCECodeName>PA</MCECodeName>
						<Datatype>double</Datatype>
						<Unit>mmHg</Unit>
						<DefaultValue>true</DefaultValue>
					</SensorDataType>
				</Create>
				<Create>
					<SensorDataType>
						<Name>SoilMoisture</Name>
						<MCECodeName>SOIL</MCECodeName>
						<Datatype>double</Datatype>
						<Unit>kPa</Unit>
						<DefaultValue>true</DefaultValue>
					</SensorDataType>
				</Create>
			</QueryString>
		</executeQuery>
	</Result>
</Macro>
